pipeline {
    agent any
    tools {
        nodejs '19.2.0'
    }
    stages {
        stage('Build') { 
            steps {
                sh 'npm install @myle1003/clothing-store-manager'
            }
        }
    }

    post {
        success {
            echo "SUCCESSFUL"
        }
        failure {
            echo "FAILED"
        }
    }
}